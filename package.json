{
  "name": "@variousjs/create",
  "version": "3.3.3",
  "description": "VariousJS boilerplate",
  "bin": "bin/create.js",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint": "tsc --skipLibCheck --noemit && eslint . --ext .ts,.tsx,.js",
    "start": "NODE_ENV=development webpack --progress --config ./webpack/component.js & NODE_ENV=development webpack serve --config ./webpack/app.js --progress",
    "build": "webpack --progress --config ./webpack/base.js",
    "prepublishOnly": "node bin/copy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/variousjs/boilerplate.git"
  },
  "keywords": [
    "template",
    "variousjs"
  ],
  "author": "LoeiFy <LoeiFy@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/variousjs/boilerplate/issues"
  },
  "homepage": "https://github.com/variousjs/boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.19.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@variousjs/registry": "^0.1.9",
    "@variousjs/various": "^5.1.5",
    "babel-loader": "^8.2.2",
    "core-js": "^3.25.1",
    "css-loader": "^6.3.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "style-loader": "^3.3.0",
    "typescript": "^5.4.2",
    "webpack": "^5.78.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.7.3"
  }
}
